<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/clean_bg">
     <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
         android:id="@+id/mSwipeRefreshLayout"
         android:layout_width="match_parent"
         android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/rl_login"
            android:layout_width="460dp"
            android:layout_height="70dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="265dp"
            android:background="@drawable/user_background"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/img_head"
                android:layout_width="26dp"
                android:layout_height="37dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="15dp"
                android:src="@drawable/ic_user" />

            <TextView
                android:id="@+id/tv_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/img_head"
                android:text="@string/login_tips"
                android:textColor="#ffffff"
                android:textSize="28sp" />

            <ImageView
                android:id="@+id/img_down"
                android:layout_width="27dp"
                android:layout_height="13dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="23dp"
                android:scaleType="fitXY"
                android:src="@drawable/down" />
        </RelativeLayout>

        <com.deepblue.cleaning.cleanview.MaxHeightRecyclerView
            android:id="@+id/rv_users"
            android:layout_width="460dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="342dp"
            android:background="@drawable/user_background"
            android:scrollbarSize="@dimen/sw_4dp"
            android:scrollbarThumbVertical="@color/colorcommon"
            android:scrollbars="vertical"
            android:visibility="gone"
            app:maxHeight="@dimen/sw_365dp" />

        <RelativeLayout
            android:id="@+id/rl_pw"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/sw_60dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="348dp"
            android:descendantFocusability="blocksDescendants"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_pw"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:id="@+id/rl_pwone"
                    android:layout_width="73dp"
                    android:layout_height="@dimen/sw_60dp"
                    android:layout_marginRight="@dimen/sw_60dp">

                    <EditText
                        android:id="@+id/btn_pwone"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_margin="@dimen/sw_6dp"
                        android:background="@null"
                        android:enabled="false"
                        android:gravity="center_horizontal"
                        android:inputType="numberPassword"
                        android:maxLength="1"
                        android:textColor="#ffffff"
                        android:textCursorDrawable="@drawable/cursor_purl"
                        android:textSize="36sp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/sw_2dp"
                        android:layout_alignParentBottom="true"
                        android:background="#8FAADE" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_pwtwo"
                    android:layout_width="73dp"
                    android:layout_height="@dimen/sw_60dp"
                    android:layout_marginRight="56dp">

                    <EditText
                        android:id="@+id/btn_pwtwo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_margin="@dimen/sw_6dp"
                        android:background="@null"
                        android:enabled="false"
                        android:gravity="center_horizontal"
                        android:inputType="numberPassword"
                        android:maxLength="1"
                        android:textColor="#ffffff "
                        android:textCursorDrawable="@drawable/cursor_purl"
                        android:textSize="36sp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/sw_2dp"
                        android:layout_alignParentBottom="true"
                        android:background="#8FAADE" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_pwthree"
                    android:layout_width="73dp"
                    android:layout_height="@dimen/sw_60dp"
                    android:layout_marginRight="56dp">

                    <EditText
                        android:id="@+id/btn_pwthree"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_margin="@dimen/sw_6dp"
                        android:background="@null"
                        android:enabled="false"
                        android:gravity="center_horizontal"
                        android:inputType="numberPassword"
                        android:maxLength="1"
                        android:textColor="#ffffff"
                        android:textCursorDrawable="@drawable/cursor_purl"
                        android:textSize="36sp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/sw_2dp"
                        android:layout_alignParentBottom="true"
                        android:background="#8FAADE" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_pwfour"
                    android:layout_width="73dp"
                    android:layout_height="@dimen/sw_60dp">

                    <EditText
                        android:id="@+id/btn_pwfour"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_margin="@dimen/sw_6dp"
                        android:background="@null"
                        android:enabled="false"
                        android:gravity="center_horizontal"
                        android:inputType="numberPassword"
                        android:maxLength="1"
                        android:textColor="#ffffff"
                        android:textCursorDrawable="@drawable/cursor_purl"
                        android:textSize="36sp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/sw_2dp"
                        android:layout_alignParentBottom="true"
                        android:background="#8FAADE" />
                </RelativeLayout>
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/rl_password"
                android:layout_width="@dimen/sw_410dp"
                android:layout_height="@dimen/sw_60dp" />
        </RelativeLayout>

        <Button
            android:id="@+id/rl_button"
            android:layout_width="200dp"
            android:layout_height="90dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="126dp"
            android:background="@drawable/dialog_okbutton"
            android:text="@string/login"
            android:textColor="#ffffff"
            android:textSize="30sp" />

        <LinearLayout
            android:id="@+id/ll_keyboard"
            android:layout_width="458dp"
            android:layout_height="300dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:background="#4E7ED9"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/sw_0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <RelativeLayout
                    android:id="@+id/rl_one"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="1"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="@dimen/sw_1dp"
                    android:layout_height="match_parent"
                    android:background="#33ffffff" />

                <RelativeLayout
                    android:id="@+id/rl_two"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="2"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="@dimen/sw_1dp"
                    android:layout_height="match_parent"
                    android:background="#33ffffff" />

                <RelativeLayout
                    android:id="@+id/rl_three"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="3"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/sw_1dp"
                android:background="#33ffffff" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/sw_0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <RelativeLayout
                    android:id="@+id/rl_four"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_four"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="4"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="@dimen/sw_1dp"
                    android:layout_height="match_parent"
                    android:background="#33ffffff" />

                <RelativeLayout
                    android:id="@+id/rl_five"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_five"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="5"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="@dimen/sw_1dp"
                    android:layout_height="match_parent"
                    android:background="#33ffffff" />

                <RelativeLayout
                    android:id="@+id/rl_six"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_six"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="6"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/sw_1dp"
                android:background="#33ffffff" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/sw_0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <RelativeLayout
                    android:id="@+id/rl_seven"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_seven"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="7"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="@dimen/sw_1dp"
                    android:layout_height="match_parent"
                    android:background="#33ffffff" />

                <RelativeLayout
                    android:id="@+id/rl_eight"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_eight"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="8"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="@dimen/sw_1dp"
                    android:layout_height="match_parent"
                    android:background="#33ffffff" />

                <RelativeLayout
                    android:id="@+id/rl_nine"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_nine"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="9"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/sw_1dp"
                android:background="#33ffffff" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/sw_0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <RelativeLayout
                    android:id="@+id/rl_cancle"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_cancle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="@android:string/cancel"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="24sp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="@dimen/sw_1dp"
                    android:layout_height="match_parent"
                    android:background="#33ffffff" />

                <RelativeLayout
                    android:id="@+id/rl_zero"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/tv_zero"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="0"
                        android:textColor="@color/sel_tvkeyboard"
                        android:textSize="30sp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="@dimen/sw_1dp"
                    android:layout_height="match_parent"
                    android:background="#33ffffff" />

                <RelativeLayout
                    android:id="@+id/rl_back"
                    android:layout_width="@dimen/sw_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:src="@drawable/ic_shape" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
     </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
</RelativeLayout>

